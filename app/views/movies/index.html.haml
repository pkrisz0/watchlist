= render'shared/logo'

  %table
    %tbody
      - @movies.each do |movie|
        .fbox{title: "#{ 'movie No.' + movie.id.to_s }"}
          %img{ height: "280", src: movie.poster, width:"190"}/
          .fsub
            %ul
              %li= movie.title
              %li= movie.year
              %li= movie.director_name
              %li= movie.rating
